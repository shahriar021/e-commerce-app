{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@native-env": [
        "./env.d.ts"
      ]
    }
  },
  "include": [
    "env.d.ts",
    "src/**/*",
    "nativewind-env.d.ts",
    "*.d.ts"
  ]
}